    <!-- about
    ================================================== -->
    <section id='about' class="s-about">

        <div class="row section-header" data-aos="fade-up">
            <div class="col-full">
                <h3 class="subhead">Lista</h3>
                <h1 class="display-1">Listado de Clientes.</h1>
            </div>
        </div> <!-- end section-header -->

        <div class="row" data-aos="fade-up">
            @using CasoExamen.Negocio;

            <div align="justify" class="col-full">
                <p class="lead">


                    @{
                        ViewBag.Title = "Index";
                    }

                    @{ List<Cliente> clientes = (List<Cliente>)ViewBag.clientes;}

                    <button> @Html.ActionLink("Crear Cliente", "Create", "Cliente")</button>

                    <div id="caja" align="justify">


                        <table align="center">
                            <tr>
                                <th>Rut</th>
                                <th>Nombre Empresa</th>
                                <th>Nombre Representante</th>
                                <th>Apellido Representante</th>
                                <th>Correo</th>
                                <th>Contraseña</th>

                            </tr>
                            @foreach (var cliente in clientes)
                            {
                                <tr>
                                    <td>@cliente.Rut</td>
                                    <td>@cliente.NomEmpresa</td>
                                    <td>@cliente.NomRepre</td>
                                    <td>@cliente.ApeRepre</td>
                                    <td>@cliente.Correo</td>
                                    <td>@cliente.Password</td>


                                </tr>

                            }

                        </table>
                        <table align="center">
                            <tr>
                                <th>Direccion</th>
                                <th>Numero Telefono</th>
                                <th>Rubro</th>
                                <th>Opciones</th>
                            </tr>
                            @foreach (var cliente in clientes)
                            {
                                <tr>
                                    <td>@cliente.Direccion</td>
                                    <td>@cliente.Telefono</td>
                                    <td>@cliente.Rubro.Nombre</td>
                                    <td>
                                        @Html.ActionLink("Eliminar", "Delete", new { id = cliente.Id })
                                        @Html.ActionLink("Modificar", "Edit", new { id = cliente.Id })
                                    </td>
                                </tr>

                            }

                        </table>




                    </div>

                    @TempData["mensaje"]
                </p>

            </div>
        </div> <!-- end about-desc -->




        <div class="container">
            <div class="row">
                <h2>Contactanos</h2>
            </div>
            <div class="row">
                <h4 style="text-align:center">Si necesitas contactarnos lo puedes hacer a traves del siguiente formulario!</h4>
            </div>
            <div class="row input-container">
                <div class="col-xs-12">
                    <div class="styled-input wide">
                        <input type="text" required />
                        <label>Nombre</label>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12">
                    <div class="styled-input">
                        <input type="text" required />
                        <label>Correo Electronico</label>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12">
                    <div class="styled-input" style="float:right;">
                        <input type="text" required />
                        <label>Telefono</label>
                    </div>
                </div>
                <div>
                    <div>
                        <label>Mensaje</label>
                        <textarea>Escriba su mensaje</textarea>
                    </div>
                </div>
                <div class="col-xs-12">
                    <div class="btn-lrg submit-btn">Enviar Formulario</div>
                </div>
            </div>
        </div>
