    <!-- home
    ================================================== -->
    <section id="home" class="s-home target-section" data-parallax="scroll" data-image-src="Models/fondo1.jpg" data-natural-width=3000 data-natural-height=2000 data-position-y=top>

        <div class="shadow-overlay"></div>

        <div class="home-content">

            <div class="row home-content__main">
                <h1>
                    Security
                </h1>

                <p>
                    Protegiendote en   <br>
                    todo lugar.
                </p>
            </div> <!-- end home-content__main -->

        </div> <!-- end home-content -->

        <ul class="home-sidelinks">
            <li>@Html.ActionLink("Administrador", "Login", "Auth")
            <li>@Html.ActionLink("Profesionales", "Index", "Profesional")
            <li>@Html.ActionLink("Clientes", "Index", "Cliente")
        </ul> <!-- end home-sidelinks -->





    </section> <!-- end s-home -->
    <!-- about
    ================================================== -->
    <center>
        <section id='about' class="s-about">

            <div class="row section-header" data-aos="fade-up">
                <div class="col-full">
                    <h3 class="subhead" align="left">Seguridad</h3>
                    <h1 class="display-1" align="left">Listado de Capacitaciones.</h1>
                </div>
            </div> <!-- end section-header -->

            <div class="row" data-aos="fade-up">
                <div align="justify" class="col-full">
                    <p class="lead">








                        @using CasoExamen.Negocio
                        @{
                            ViewBag.Title = "Index";
                        }

                        

                        @{List<Capacitacion> capacitaciones = (List<Capacitacion>)ViewBag.capacitaciones;}
                       
                        <button>@Html.ActionLink("Crear Capacitacion", "Create")</button>
                        <table>
                            <tr>
                                <th>Fecha</th>
                                <th>Asistentes</th>
                                <th>Descripcion</th>
                            </tr>
                            @foreach (var capacitacion in capacitaciones)
                            {
                                <tr>
                                    <td>@capacitacion.Fecha</td>
                                    <td>@capacitacion.Asistentes</td>
                                    <td>@capacitacion.Descripcion</td>
                                </tr>
                            }
                        </table>
                        @TempData["mensaje"]



                    </p>

                </div>
            </div> <!-- end about-desc -->






        </section> <!-- end s-about -->




    </center>

    <!-- services
    ================================================== -->
    <section id='services' class="s-services light-gray">

        <div class="container">
            <div class="row">
                <h2>Contactanos</h2>
            </div>
            <div class="row">
                <h4 style="text-align:center">Si necesitas contactarnos lo puedes hacer a traves del siguiente formulario!</h4>
            </div>
            <div class="row input-container">
                <div class="col-xs-12">
                    <div class="styled-input wide">
                        <input type="text" required />
                        <label>Nombre</label>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12">
                    <div class="styled-input">
                        <input type="text" required />
                        <label>Correo Electronico</label>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12">
                    <div class="styled-input" style="float:right;">
                        <input type="text" required />
                        <label>Telefono</label>
                    </div>
                </div>
                <div>
                    <div>
                        <label>Mensaje</label>
                        <textarea>Escriba su mensaje</textarea>
                    </div>
                </div>
                <div class="col-xs-12">
                    <div class="btn-lrg submit-btn">Enviar Formulario</div>
                </div>
            </div>
        </div>



    </section> <!-- end s-services -->
    <!-- contact
    ================================================== -->
    <section id="contact" class="s-contact">

